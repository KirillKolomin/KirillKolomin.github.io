{
  "extends": [
    "next/core-web-vitals",
    "airbnb"
  ],
  "rules": {
    "react/jsx-filename-extension": [
      2,
      {
        "extensions": [
          ".jsx",
          ".tsx"
        ]
      }
    ],
    "consistent-return": "off",
    "no-param-reassign": "off",
    "no-restricted-globals": ["error", "event", "fdescribe"]
  }
}
